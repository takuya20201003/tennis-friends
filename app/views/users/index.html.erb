<h1>
  検索
</h1>
<%= search_form_for @u, url: users_search_path do |f| %>
  <%= f.label :name_eq, 'フリーワード' %>
  <%= f.collection_select :name_eq, @users, :name, :name, include_blank: '指定なし' %>
  <br>
  <%= f.submit '検索' %>
<% end %>

<%# 検索結果一覧 %>
<% @users.each do |user| %>
  <br>
  <li>
    <%= user.name %>
  </li>
<% end %>